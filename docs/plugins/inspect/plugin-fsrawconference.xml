<plugin>
  <name>fsrawconference</name>
  <description>Farsight Raw Conference plugin</description>
  <filename>../../gst/fsrawconference/.libs/libfsrawconference.so</filename>
  <basename>libfsrawconference.so</basename>
  <version>0.0.21.1</version>
  <license>LGPL</license>
  <source>farsight2</source>
  <package>Farsight</package>
  <origin>http://farsight.freedesktop.org/</origin>
  <elements>
    <element>
      <name>fsrawconference</name>
      <longname>Generic bin</longname>
      <class>Generic/Bin</class>
      <description>Simple container object</description>
      <author>Erik Walthinsen &lt;omega@cse.ogi.edu&gt;,Wim Taymans &lt;wim.taymans@gmail.com&gt;</author>
      <pads>
        <caps>
          <name>sink_%d</name>
          <direction>sink</direction>
          <presence>sometimes</presence>
          <details>ANY</details>
        </caps>
        <caps>
          <name>src_%d</name>
          <direction>source</direction>
          <presence>sometimes</presence>
          <details>ANY</details>
        </caps>
      </pads>
    </element>
  </elements>
</plugin>